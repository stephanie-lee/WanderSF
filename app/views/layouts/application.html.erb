<!DOCTYPE html>
<html>
<head>
  <title>WanderSF</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= csrf_meta_tags %>
  <%= javascript_include_tag 'application' %>
  <link href='https://fonts.googleapis.com/css?family=Roboto:400,100,300italic,300,700' rel='stylesheet' type='text/css'>
  <script src="http://widget.cloudinary.com/global/all.js" type="text/javascript">
  </script>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAe7wJ7_QURxvvX7qt0yMQWoDEaGZvzQpQ"
          type="text/javascript"></script>

  </head>
<body>
  <% if current_user %>
    <script>
  	window.CURRENT_USER_ID = <%= current_user.id %>
    </script>
  <% end %>

  <%= render partial: "shared/errors" %>
    <%= yield %>
  <%= render partial: "shared/footer" %>
</body>
</html>
